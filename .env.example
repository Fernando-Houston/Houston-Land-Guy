# Environment
NODE_ENV=production

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/database_name
REDIS_URL=redis://localhost:6379

# Core Agents Integration
CORE_AGENTS_URL=https://your-core-agents-url.com
CORE_AGENTS_SECRET_KEY=your-core-agents-secret-key

# NextAuth Configuration (REQUIRED)
NEXTAUTH_SECRET=generate-a-random-secret-key-here
NEXTAUTH_URL=https://your-domain.com

# External APIs
PERPLEXITY_API_KEY=your-perplexity-api-key
CENSUS_API_KEY=your-census-api-key
REPLICATE_API_TOKEN=your-replicate-api-token

# Real Estate Data APIs
REAL_ESTATE_API_URL=https://api.houstonrealestate.com
REAL_ESTATE_API_KEY=your-har-mls-api-key
HOUSTON_PERMITS_API_KEY=your-houston-permits-api-key
HARRIS_COUNTY_API_KEY=your-harris-county-api-key
ECONOMIC_DATA_API_KEY=your-economic-data-api-key

# AI & Machine Learning
OPENAI_API_KEY=your-openai-api-key
CLAUDE_API_KEY=your-claude-api-key
AZURE_OPENAI_ENDPOINT=your-azure-endpoint
AZURE_OPENAI_API_KEY=your-azure-api-key

# Google Maps API (NEVER COMMIT THE ACTUAL KEY)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Email Service
RESEND_API_KEY=your-resend-api-key
RESEND_FROM_EMAIL=noreply@yourdomain.com

# Feature Flags
USE_REPLICATE=false